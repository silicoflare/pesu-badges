{
    "name": "pesu-badges",
    "builds": [
        {
            "src": "/frontend/**",
            "use": "@vercel/static-build"
        }, {
            "src": "/backend/**",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/backend/$1"
        }, {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        }
    ]
}
